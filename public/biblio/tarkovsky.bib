@article{schmidtSomatographyFilmNostalgia2016,
  title      = {Somatography and {{Film}}: {{Nostalgia}} as {{Haunting Memory Shown}} in {{Tarkovsky}}'s {{{\emph{Nostalghia}}}}},
  shorttitle = {Somatography and {{Film}}},
  author     = {Schmidt, Stefan W.},
  year       = {2016},
  month      = jan,
